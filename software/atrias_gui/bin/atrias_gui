#!/usr/bin/env python

import os
import sys

try:
    import rospkg
    r = rospkg.RosPack()
    p = r.get_path('atrias_gui')
    sys.path.append(os.path.join(p, 'src'))
except ImportError:
    print ""
    print "FATAL: Cannot import the rospkg python library! Cannot continue."
    print ""
    sys.exit(1)

from atrias_gui.magic import magic
sys.exit(magic(__file__))